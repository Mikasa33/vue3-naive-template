{
  "name": "vue3-admin-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env VITE_ENV_TYPE=dev vite",
    "dev:prod": "cross-env VITE_ENV_TYPE=prod vite",
    "build": "npm run typecheck && cross-env VITE_ENV_TYPE=prod vite build",
    "build:dev": "npm run typecheck && cross-env VITE_ENV_TYPE=dev vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vueuse/core": "^8.3.1",
    "@vueuse/integrations": "^8.3.1",
    "axios": "^0.27.2",
    "dayjs": "^1.11.1",
    "lodash": "^4.17.21",
    "pinia": "^2.0.13",
    "qs": "^6.10.3",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.34",
    "@iconify/vue": "^3.2.1",
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/node": "^16.11.32",
    "@unocss/reset": "^0.32.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-plugin-vue": "^8.7.1",
    "less": "^4.1.2",
    "mockjs": "^1.1.0",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "typescript": "~4.6.4",
    "unocss": "^0.31.17",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.6",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-html-template": "^1.1.2",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^0.34.11"
  }
}
